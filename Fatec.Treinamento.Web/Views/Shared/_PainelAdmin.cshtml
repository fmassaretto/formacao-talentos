
@{
    ViewBag.Title = "Painel do Administrador";
}

<div class="painel-title">
    <div class="painel-saudacao">
        <h2>Seja bem vindo ao seu painel, @this.User.Identity.Name!</h2>
    </div>
</div>
<div class="painel-body">
    <div class="vSpacer"></div>
    <div class="painel-main">
        <div id="gerenciar-container">

        <div class="gerenciar-container-item">
            <ul class="nav nav-pills nav-stacked">
                <li role="presentation"><a href="@Url.Action("Redirecionador", "Admin", new { toPage = "Usuarios" })" class="menu-item">Gerenciar Usuários</a></li>
                <li role="presentation"><a href="@Url.Action("Redirecionador", "Admin", new { toPage = "Cursos" })" class="menu-item">Gerenciar Curso</a></li>
                <li role="presentation"><a href="@Url.Action("Redirecionador", "Admin", new { toPage = "Assuntos" })" class="menu-item">Gerenciar Assuntos</a></li>
                <li role="presentation"><a href="@Url.Action("Redirecionador", "Admin", new { toPage = "Trilhas" })" class="menu-item">Gerenciar Trilhas</a></li>
                <li role="presentation"><a href="@Url.Action("Redirecionador", "Admin", new { toPage = "Videos" })" class="menu-item">Gerenciar Videos</a></li>
            </ul>            
        </div>
            <div class="gerenciar-container-item">
                <div class="gerenciar-content-title">
                    <h3>Visão Geral</h3>
                    <hr />
                </div> 
                <div class="painel-content-body">

                </div>   
            </div>
        </div>

    </div>

</div>



