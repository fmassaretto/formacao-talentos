
@{
    ViewBag.Title = "Painel do Administrador";
}

<div class="painel-title">
    <div class="painel-saudacao">
        <h2>Seja bem vindo ao seu painel, @this.User.Identity.Name!</h2>
    </div>
</div>
<div class="painel-body">
    <div class="vSpacer"></div>
    <div class="painel-main">
        <div class="gerenciar-container">

        <div class="menu-gerenciar-side">
            <ul class="nav nav-pills nav-stacked">
                <li role="presentation"><a href="@Url.Action("Redirecionador", "Admin", new { toPage = "Usuarios" })">Gerenciar Usuários</a></li>
                <li role="presentation"><a href="@Url.Action("Redirecionador", "Admin", new { toPage = "Cursos" })">Gerenciar Curso</a></li>
                <li role="presentation"><a href="@Url.Action("Redirecionador", "Admin", new { toPage = "Assuntos" })">Gerenciar Assuntos</a></li>
                <li role="presentation"><a href="@Url.Action("Redirecionador", "Admin", new { toPage = "Trilhas" })">Gerenciar Trilhas</a></li>
                <li role="presentation"><a href="@Url.Action("Redirecionador", "Admin", new { toPage = "Videos" })">Gerenciar Videos</a></li>
            </ul>            
        </div>
            <section class="gerenciar-content">
                <div class="gerenciar-content-title">
                    <h3>Visão Geral</h3>
                    <hr />
                </div> 
                <div class="painel-content-body">

                </div>   
            </section>
        </div>

    </div>

</div>



